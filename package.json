{
	"name": "unplugin-glob-entry",
	"type": "module",
	"version": "0.1.0",
	"packageManager": "pnpm@9.12.2",
	"description": "",
	"license": "MIT",
	"homepage": "https://github.com/antfu/unplugin-glob-entry#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/antfu/unplugin-glob-entry.git"
	},
	"bugs": {
		"url": "https://github.com/antfu/unplugin-glob-entry/issues"
	},
	"keywords": [
		"unplugin",
		"vite",
		"webpack",
		"rollup",
		"transform"
	],
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./astro": {
			"import": "./dist/astro.js",
			"require": "./dist/astro.cjs"
		},
		"./rspack": {
			"import": "./dist/rspack.js",
			"require": "./dist/rspack.cjs"
		},
		"./vite": {
			"import": "./dist/vite.js",
			"require": "./dist/vite.cjs"
		},
		"./webpack": {
			"import": "./dist/webpack.js",
			"require": "./dist/webpack.cjs"
		},
		"./rollup": {
			"import": "./dist/rollup.js",
			"require": "./dist/rollup.cjs"
		},
		"./nuxt": {
			"import": "./dist/nuxt.js",
			"require": "./dist/nuxt.cjs"
		},
		"./farm": {
			"import": "./dist/farm.js",
			"require": "./dist/farm.cjs"
		},
		"./types": {
			"import": "./dist/types.js",
			"require": "./dist/types.cjs"
		},
		"./*": "./*"
	},
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*",
				"./*"
			]
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch src",
		"build:fix": "esno scripts/postbuild.ts",
		"lint": "biome lint .",
		"format": "biome format --write .",
		"prepublishOnly": "npm run build",
		"release": "bumpp && npm publish",
		"start": "esno src/index.ts",
		"test": "vitest"
	},
	"peerDependencies": {
		"@farmfe/core": ">=1",
		"@nuxt/kit": "^3",
		"@nuxt/schema": "^3",
		"rollup": "^3",
		"vite": ">=3",
		"webpack": "^4 || ^5"
	},
	"peerDependenciesMeta": {
		"@farmfe/core": {
			"optional": true
		},
		"@nuxt/kit": {
			"optional": true
		},
		"@nuxt/schema": {
			"optional": true
		},
		"rollup": {
			"optional": true
		},
		"vite": {
			"optional": true
		},
		"webpack": {
			"optional": true
		}
	},
	"dependencies": {
		"globby": "^14.0.2"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@nuxt/kit": "^3.13.2",
		"@nuxt/schema": "^3.13.2",
		"@rspack/core": "^1.0.14",
		"@types/node": "^22.8.1",
		"@vitest/coverage-v8": "^2.1.3",
		"bumpp": "^9.7.1",
		"chalk": "^5.3.0",
		"es-module-lexer": "^1.5.4",
		"esno": "^4.8.0",
		"fast-glob": "^3.3.2",
		"memfs": "^4.14.0",
		"nodemon": "^3.1.7",
		"prettier": "^3.3.3",
		"rolldown": "^0.13.2",
		"rollup": "^4.24.0",
		"tsup": "^8.3.5",
		"typescript": "^5.6.3",
		"unplugin": "^1.14.1",
		"vite": "^5.4.10",
		"vitest": "^2.1.3",
		"webpack": "^5.95.0",
		"webpack-merge": "^6.0.1"
	}
}
